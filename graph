digraph {
	node [height=0.1 shape=box]
	edge [arrowsize=0.5]
	node1 [label=Program]
	node2 [label=Main]
	node1 -> node2
	node3 [label=Block]
	node2 -> node3
	node4 [label=FuncCall]
	node3 -> node4
	node5 [label="Id : writeln"]
	node4 -> node5
	node6 [label=Args]
	node4 -> node6
	node7 [label="String : Hello world!"]
	node6 -> node7
}
